{
    "name": "kamansoft/laravel-multiorg",
    "description": "laravel package to allow multiorganizational",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kamansoft\\LaravelMultiorg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamansoft\\LaravelMultiorg\\": "tests/"
        }
