        }
    },
    "authors": [{
        "name": "lemys lopez",
        "email": "lemyskaman@gmail.com"
    }],
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "dyrynda/laravel-model-uuid": "^6.6"
    }
